AlgoritmO "teste_enquanto"

VaR
    x, soma: InteirO

IniciO

      Escreva("Digite o primeiro número:")
      Leia(x)
      
      soma <- 0
      
      EnquantO x <> 0 Faca
       soma <- soma + x
        Escreva("Digite outro número:")
        Leia(x)
      FimEnquantO
      
      EscrevaL("SOMA = ",soma)

FimAlgoritmO
