AlgoritmO "dardo"

VaR

    d1, d2, d3, maior: ReaL
    
IniciO

   Escreva("Digite a três distância:")
   Leia(d1)
   Leia(d2)
   Leia(d3)
   
   Se (d1 > d2) e (d1 > d3) EntaO
      maior <- d1
   Senao
      Se d2 > d3 EntaO
      maior <- d2
      Senao
      maior <- d3
   Fimse
  Fimse
  
  Escreva("MAIOR DISTANCIA = ", maior:4:2)

FimAlgoritmO

